exec > api/python/package/OpenPanel/error.py
echo '# This file is part of the OpenPanel API'
echo '# It is autogenerated, do not edit'
perl -ne '
if (m!^#define\s+(ERR_[^ ]+)\s+(0x[0-9a-f]+)\s*//\s*(.*)!)
{
	print "$1 = $2\n";
}' < error.h
